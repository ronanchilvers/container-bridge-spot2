    "description": "Bridge for integrating vlucas/spot2 with ronanchilvers/container",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ronan Chilvers",
            "email": "ronan@thelittledot.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "vlucas/spot2": "^2.2",
        "ronanchilvers/container": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
